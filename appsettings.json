{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:3000"
      }
    }
  },
  {
    "ConnectionStrings": {
      "DefaultConnection": "Data Source=localhost,1440;Initial Catalog=MovieDB;Persist Security Info=True;"
    },
    "AccessTokenManagement": {
      "Domain": "{DOMAIN}",
      "Clients": [
        {
          "Name": "UserService",
          "ClientId": "{CLIENT_ID}",
          "ClientSecret": "{CLIENT_SECRET}",
          "BaseUri": "{BASE_URI}"
        }
      ]
    },
    "Auth0": {
      "ManagementApi": null,
      "BaseUri": "{BASE_URI}",
      "Domain": "{DOMAIN}",
      "ClientId": "{CLIENT_ID}",
      "ClientSecret": "{CLIENT_SECRET}"
    }
  }
